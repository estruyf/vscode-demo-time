{
  "name": "demo-time",
  "version": "1.11.0",
  "private": true,
  "description": "Demo Time - The extension to present and perform live coding demos with confidence.",
  "keywords": [
    "demo",
    "presentation",
    "slides",
    "coding demo",
    "workshop",
    "training",
    "automation",
    "scripted demo",
    "presenter",
    "conference",
    "tutorial",
    "education",
    "teaching",
    "live coding",
    "presentation mode",
    "slides viewer",
    "demo runner",
    "step-by-step",
    "highlighting",
    "presenter view",
    "timer",
    "countdown",
    "notes",
    "pdf export",
    "powerpoint",
    "markdown slides"
  ],
  "scripts": {
    "docs:dev": "npx nx dev demo-time-docs",
    "docs:build": "npx nx build demo-time-docs",
    "common:build": "npx nx build common",
    "common:dev": "npx nx run common:dev",
    "webviews:build": "npx nx build webviews",
    "webviews:dev": "npx nx run webviews:dev",
    "vscode:dev": "npx nx run-many --target=dev --projects=vscode-demo-time,webviews,common --parallel",
    "vscode:build": "npx nx build vscode-demo-time",
    "test": "npx nx run-many -t test",
    "lint": "npx nx run-many -t lint",
    "sync:version:extension": "node ./scripts/sync-version-to-extension.js",
    "postversion": "node ./scripts/sync-version-to-extension.js"
  },
  "homepage": "https://demotime.show/",
  "directories": {
    "doc": "docs",
    "example": "examples"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/estruyf"
  },
  "author": "Elio Struyf <elio@struyfconsulting.be> (https://www.eliostruyf.com)",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "packages/*",
    "docs"
  ],
  "devDependencies": {
    "@actions/core": "^1.11.1",
    "@nx/eslint": "21.4.0",
    "@nx/jest": "21.4.0",
    "@nx/js": "^21.4.0",
    "@nx/web": "21.2.3",
    "@nx/workspace": "^21.4.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.33.0",
    "jest": "^29.7.0",
    "nx": "21.4.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0"
  },
  "packageManager": "yarn@4.9.4"
}
